{
  "name": "@prarthana25/teams-cli",
  "version": "1.0.7",
  "description": "A comprehensive CLI tool for managing teams, team members, repositories, and analyzing team activity with GitHub OAuth integration",
  "main": "dist/index.js",
  "bin": {
    "teams": "dist/index.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "pub": "npm publish --access public",
    "pub:beta": "npm publish --tag beta --access public",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "test": "jest",
    "migrate:generate": "drizzle-kit generate --schema src/db/schema.ts --out migrations",
    "migrate:push": "drizzle-kit push --connection sqlite:./teams.db",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitoriiiiiiii/Teams-Cli.git"
  },
  "homepage": "https://github.com/hitoriiiiiiii/Teams-Cli#readme",
  "bugs": {
    "url": "https://github.com/hitoriiiiiiii/Teams-Cli/issues"
  },
  "keywords": [
    "cli",
    "teams",
    "team-management",
    "github",
    "oauth",
    "members",
    "invites",
    "analytics",
    "productivity",
    "collaboration",
    "repository-management"
  ],
  "author": "hitoriiiiiiii (https://github.com/hitoriiiiiiii)",
  "license": "MIT",
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.6.2",
    "chalk": "^4.1.2",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "inquirer": "^8.2.7",
    "node-cron": "^4.2.1",
    "open": "^8.4.2",
    "ora": "^5.4.1",
    "redis": "^4.7.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/express-rate-limit": "^5.1.3",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "prettier": "^3.8.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "types": "./dist/index.d.ts"
}
